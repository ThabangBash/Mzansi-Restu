<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="100dp"
        />
    <gradient
        android:angle="45"
        android:centerX="65%"
        android:centerColor="#C25E00"
        android:startColor="#fb8c00"
        android:endColor="#ffbd45"
        android:type="linear"
        />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp"
        />
    <size
        android:width="200dp"
        android:height="45dp"
        />
    <stroke
        android:width="3dp"
        android:color="#994A00"
        />
</shape>